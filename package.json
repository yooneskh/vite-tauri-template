{
  "name": "vite-tauri-template",
  "version": "1.2.0",
  "description": "Vite + Tauri + Vue 3 + Vuetify 3 desktop app template",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "vuetify",
    "desktop"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite --port 8080",
    "serve:native": "VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "serve:tauri": "tauri dev",
    "build:web": "vite build",
    "build": "VITE_HTTP_HANDLER=NATIVE vite build && tauri build"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@tauri-apps/api": "1.0.0-beta.3",
    "roboto-fontface": "0.10.0",
    "vue": "3.1.2",
    "vue-router": "4",
    "vuetify": "3.0.0-alpha.7",
    "ynetwork": "2.8.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.3",
    "@vitejs/plugin-vue": "1.2.3",
    "@vue/compiler-sfc": "3.1.2",
    "sass": "1.35.1",
    "sass-loader": "12.1.0",
    "vite": "2.3.8",
    "vue-cli-plugin-vuetify": "2.4.1"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/yooneskh/vite-tauri-template"
  },
  "license": "ISC"
}
